    "commonDate": {
      "tags": []
    },
    "database": {
      "tags": []
    },
    "environments": {
      "tags": []
    },
    "exception": {
      "tags": []
    },
    "feeds": {
      "tags": []
    },
    "logger": {
      "tags": []
    },
    "message": {
      "tags": []
    },
    "metadata": {
      "tags": []
    },
    "notifications": {
      "tags": []
    },
    "searches": {
      "tags": []
    },
    "users": {
      "tags": []
    }
  }
